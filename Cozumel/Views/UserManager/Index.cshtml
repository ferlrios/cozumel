@model List<Cozumel.Models.MyUser>
@{
    ViewData["Title"] = "Usuarios";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Usuarios</h1>
<table class="table white">
    <thead>
        <tr>
            <th>UserName</th>
            <th>Email</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
                <tr class="white">
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                    <td>
                        <a class="btn btn-primary" asp-controller="UserManager" asp-action="Verify" asp-route-userId="@user.Id">Verificar</a>
                    </td>
                </tr>

        }
    </tbody>
</table>